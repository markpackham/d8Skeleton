{
    "name": "drupal/drupal",
    "description": "Drupal is an open source content management platform powering millions of websites and applications.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "require": {
        "composer/installers": "^1.0.24",
        "wikimedia/composer-merge-plugin": "^1.4",
        "drupal/address": "^1.6",
        "drupal/addtoany": "^1.12",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/back_to_top": "^1.0",
        "drupal/backup_migrate": "^4.0",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/block_class": "^1.0",
        "drupal/captcha": "^1.0@beta",
        "drupal/config_filter": "^1.4",
        "drupal/config_split": "^1.4",
        "drupal/config_update": "^1.6",
        "drupal/csv_serialization": "^1.4",
        "drupal/ctools": "^3.2",
        "drupal/devel": "^2.1",
        "drupal/diff": "^1.0@RC",
        "drupal/easy_breadcrumb": "^1.8",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/editor_file": "^1.4",
        "drupal/entity_clone": "^1.0@beta",
        "drupal/entity_reference_revisions": "^1.6",
        "drupal/eu_cookie_compliance": "^1.5",
        "drupal/exclude_node_title": "^1.0@beta",
        "drupal/extlink": "^1.1",
        "drupal/field_permissions": "^1.0@RC",
        "drupal/file_mdm": "^1.1",
        "drupal/geofield": "^1.3",
        "drupal/google_analytics": "^3.0",
        "drupal/honeypot": "^1.29",
        "drupal/image_effects": "^2.3",
        "drupal/linkit": "^4.3",
        "drupal/mailsystem": "^4.1",
        "drupal/masquerade": "^2.0@beta",
        "drupal/menu_block": "^1.5",
        "drupal/metatag": "^1.8",
        "drupal/paragraphs": "^1.8",
        "drupal/password_policy": "^3.0@alpha",
        "drupal/pathauto": "^1.4",
        "drupal/podcast": "^1.0",
        "drupal/poll": "^1.2",
        "drupal/redirect": "^1.3",
        "drupal/redis": "^1.1",
        "drupal/restui": "^1.16",
        "drupal/scheduler": "^1.0",
        "drupal/search_api": "^1.12",
        "drupal/shield": "^1.2",
        "drupal/simple_gmap": "^1.4",
        "drupal/simple_sitemap": "^3.1",
        "drupal/sitemap": "^1.5",
        "drupal/superfish": "^1.3",
        "drupal/token": "^1.5",
        "drupal/video": "^1.4",
        "drupal/video_embed_field": "^2.0",
        "drupal/view_unpublished": "^1.0@alpha",
        "drupal/views_accordion": "^1.2",
        "drupal/views_data_export": "^1.0@beta",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/views_slideshow": "^4.6",
        "drupal/viewsreference": "^1.4",
        "drupal/webform": "^5.2",
        "drupal/weight": "^3.1",
        "drupal/adminimal_theme": "^1.4",
        "drupal/bootstrap": "^3.19",
        "drupal/google_tag": "^1.1",
        "drupal/node_revision_delete": "^1.0@alpha",
        "drupal/migrate_tools": "^4.1",
        "drupal/migrate_plus": "^4.2",
        "drupal/advagg": "^4.0",
        "drupal/coder": "^8.3",
        "drupal/url_embed": "^1.0@alpha",
        "drupal/quicklink": "^1.2",
        "drupal/field_defaults": "^1.1",
        "drupal/admin_toolbar": "^2.0",
        "drupal/yoast_seo": "^1.5"
    },
    "replace": {
        "drupal/core": "^8.7"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "autoloader-suffix": "Drupal8"
    },
    "extra": {
        "_readme": [
            "By default Drupal loads the autoloader from ./vendor/autoload.php.",
            "To change the autoloader you can edit ./autoload.php.",
            "This file specifies the packages.drupal.org repository.",
            "You can read more about this composer repository at:",
            "https://www.drupal.org/node/2718229"
        ],
        "merge-plugin": {
            "include": [
                "core/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-extra": false
        },
        "installer-paths": {
            "core": ["type:drupal-core"],
            "modules/contrib/{$name}": ["type:drupal-module"],
            "profiles/contrib/{$name}": ["type:drupal-profile"],
            "themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"],
            "modules/custom/{$name}": ["type:drupal-custom-module"],
            "themes/custom/{$name}": ["type:drupal-custom-theme"]
        }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Core\\Composer\\": "core/lib/Drupal/Core/Composer"
        }
    },
    "scripts": {
        "pre-autoload-dump": "Drupal\\Core\\Composer\\Composer::preAutoloadDump",
        "post-autoload-dump": "Drupal\\Core\\Composer\\Composer::ensureHtaccess",
        "post-package-install": "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup",
        "post-package-update": "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup",
        "drupal-phpunit-upgrade-check": "Drupal\\Core\\Composer\\Composer::upgradePHPUnit",
        "drupal-phpunit-upgrade": "@composer update phpunit/phpunit phpspec/prophecy symfony/yaml --with-dependencies --no-progress",
        "phpcs": "phpcs --standard=core/phpcs.xml.dist --runtime-set installed_paths $($COMPOSER_BINARY config vendor-dir)/drupal/coder/coder_sniffer --",
        "phpcbf": "phpcbf --standard=core/phpcs.xml.dist --runtime-set installed_paths $($COMPOSER_BINARY config vendor-dir)/drupal/coder/coder_sniffer --"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ]
}
